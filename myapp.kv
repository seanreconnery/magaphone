<MyGrid>
    GridLayout:
        cols:1
        background_color:1,1,1,1
        size: root.width, root.height
        padding: 20

        GridLayout:
            cols:2
            padding: 40

            Ta:
                source:'trump1.png'
                size_hint: .2, .2
                background_color: 0,0,0,0  # the last zero is the critical on, make invisible
                canvas.before:
                    Color:
                        rgba: (1,1,1,1) if self.state=='normal' else (0,0,0,1)  # visual feedback of press
                    RoundedRectangle:
                        pos: self.pos
                        size: self.size
                        radius: [66,]

